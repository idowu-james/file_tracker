                        <li class="gui-folder">
                            <a>
                                <div class="gui-icon"><span class="fa fa-building"></span></div>
                                <span class="title">Departments</span>
                            </a>
                            <!--start submenu -->
                            <ul>
                                <li>                            
                                    <%= link_to '<span class="title">View Departments</span>'.html_safe, departments_path %>   
