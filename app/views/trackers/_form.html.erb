<%= simple_form_for([@tracker.nasfile , @tracker]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

	
  <%= f.input :nasfile, disabled: true, label: 'File Number', input_html: { value: @nasfile.file_number } %>
    
	
  <%= f.label(:user,"Reciever") %>
    <%= f.collection_select(:receiver_id, User.all_without([current_user]), :id, :username,
    :prompt => 'Please select Receiving User' ) %>



  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
