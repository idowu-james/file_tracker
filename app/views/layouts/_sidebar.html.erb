                                <li>                            
                                     <%= link_to '<span class="title">View All Files</span>'.html_safe, nasfiles_path %>  
                                </li>                                
                            </ul><!--end /submenu -->
                        </li><!--end /menu-li -->
                       <!-- END Files -->                       

                        <!-- BEGIN Category -->
                        <li class="gui-folder">
                            <a>
                                <div class="gui-icon"><i class="fa fa-folder-open"></i></div>
                                <span class="title">File Categories</span>
                            </a>
                            <!--start submenu -->
                            <ul>                                
                                <li>                            
                                    <%= link_to '<span class="title">View Categories</span>'.html_safe, categories_path %>  
